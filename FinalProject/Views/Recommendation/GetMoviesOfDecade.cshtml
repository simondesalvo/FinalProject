@model List<UserMovie>
@{
    ViewData["Title"] = "GetMoviesOfDecade";
}

<h4>Recommended other Movies Of the Decade</h4>
@if (Model.Count == 0)
{
    <p>No movies found!</p>
}
else
{
    foreach (UserMovie um in Model)
    {
        <a href="../Home/MovieSelection?id=@um.MovieId">@um.Title</a>
    }
}
