@model List<UserMovie>
@{
    ViewData["Title"] = "GetMoviesOfDecade";
}

<h4>Recommended other Movies Of Selected Decade</h4>
@if (Model.Count == 0)
{
    <p>No movies found!</p>
}
else
{
<table class="table table-bordered">
    @foreach (UserMovie um in Model)
    {
    <td><a href="../Home/MovieSelection?id=@um.MovieId">@um.Title</a></td>
    }
</table>
}
