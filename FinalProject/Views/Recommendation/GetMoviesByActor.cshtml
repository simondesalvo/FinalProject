@model List<DictionaryVM>

    <h4>Actors and average user scores for the movies they're in:</h4>
@if (Model.Count == 0)
{

    <p>Looks like we don't have any movies with these actors in our database, try another! </p>
}
else
{
<table class="table table-bordered">
    @foreach (DictionaryVM vm in Model)
    {
    <tr>
        <th>@vm.Actor</th>
    </tr>
        foreach (UserMovie m in vm.MoviesWithScore.Keys)
        {
            <tr>
            <td><a href="../Home/MovieSelection?id=@m.MovieId">@m.Title</a></td>
            <td>Average Score: @vm.MoviesWithScore[m]</td>
            </tr>
        }
    }
</table>
}



