@model Dictionary<UserMovie, double>
<h1>You seem to like @ViewBag.Genre movies, here are some others you might like:</h1>


@if (Model.Count == 0)
{
    <p>No Matches Found! Please search again </p>
}
else
{
    @foreach (UserMovie m in Model.Keys)
    {
        <a href="../Home/MovieSelection?id=@m.MovieId">@m.Title</a>
        <p>User Score: @Model[m]</p>


    }
}