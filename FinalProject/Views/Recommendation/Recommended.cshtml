@model Dictionary<UserMovie, double>
<h1>You have wathced a lot of @ViewBag.Genre movies, here's some other you might like</h1>


@if (Model.Count == 0)
{
    <p>No Matches Found! Please search again </p>
}
else
{
    @foreach (UserMovie m in Model.Keys)
    {
        <p>@m.Title</p>
        <p>User Score: @Model[m]</p>


    }
}