@model Dictionary<UserMovie, double>

@if (Model.Count == 0)
{
    <p>Looks like we don't have any movies with this director in our database, try another! </p>
}
@foreach (UserMovie m in Model.Keys)
{
   // <p>Directors: @ViewBag.directors.Where</p>
    <p>@m.Title</p>
    <p>Average User Score: @Model[m]</p>
}
